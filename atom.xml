---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>smallfish</title>
	<link href="http://chenxiaoyu.org/atom.xml" rel="self"/>
	<link href="http://chenxiaoyu.org"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://chenxiaoyu.org</id>
	<author>
		<name>smallfish</name>
		<email>smallfish.xy@gmail.com</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://chenxiaoyu.org{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://chenxiaoyu.org{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}

</feed>
